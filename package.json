{
  "name": "agentfund-protocol",
  "version": "0.1.0",
  "private": true,
  "description": "Self-funding infrastructure for autonomous agents on Solana",
  "repository": {
    "type": "git",
    "url": "https://github.com/tigurius/agentfund-protocol"
  },
  "author": "SatsAgent",
  "license": "MIT",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "test": "npm run test --workspaces --if-present",
    "lint": "eslint packages/*/src --ext .ts",
    "lint:fix": "eslint packages/*/src --ext .ts --fix",
    "format": "prettier --write 'packages/*/src/**/*.ts'",
    "format:check": "prettier --check 'packages/*/src/**/*.ts'",
    "clean": "rm -rf packages/*/dist packages/*/node_modules",
    "deploy:devnet": "ts-node scripts/deploy-devnet.ts",
    "deploy:mainnet": "ts-node scripts/deploy-mainnet.ts"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "prettier": "^3.2.0",
    "ts-node": "^10.9.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "solana",
    "agent",
    "ai",
    "self-funding",
    "micropayments",
    "defi",
    "bonding-curve",
    "a2a-commerce"
  ]
}
